    "@typescript-eslint/parser": "^3.10.1",
    "env-var": "^7.0.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "http-proxy-middleware": "^1.0.5",
    "prettier-eslint": "^12.0.0",
    "react-swipeable-views-utils": "^0.13.9",
    "typescript": "^3.9.7"
  }
